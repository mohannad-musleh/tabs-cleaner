{
    "manifest_version": 2,
    "name": "Tabs Cleaner",
    "description": "Clean Duplicate tabs",
    "version": "0.0.1",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_icon": "icons/icon19.png",
        "default_popup": "src/browser_action/browser_action.html"
    },
    "permissions": [
        "tabs"
    ]
}